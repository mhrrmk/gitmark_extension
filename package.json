{
  "name": "gitmark",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lowdb": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "cra-build-watch": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm-run-all --parallel dev:* postdev",
    "dev:ui": "cra-build-watch -b dev",
    "dev:backend": "webpack --config webpack.config.dev.js --watch",
    "postdev": "rsync -av src/backend dev --exclude background.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
