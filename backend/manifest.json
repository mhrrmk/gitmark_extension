{
    "manifest_version": 2,
    "name": "Gitmark",
    "version": "1.0",
    "description": "Tab manager inspired by git",
    "options_page": "/backend/options.html",
    "permissions": [
      "activeTab",
      "declarativeContent",
      "storage",
      "tabs"
    ],
    "background": {
        "scripts": "/backend/background.js",
        "persistent": false
      },
    "browser_action": {
        "default_popup": "/popup/index.html",
        "default_icon": {
            "16": "backend/images/get_started16.png",
            "32": "backend/images/get_started32.png",
            "48": "backend/images/get_started48.png",
            "128": "backend/images/get_started128.png"
      }
    }
  }
  