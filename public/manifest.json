{
  "name": "Gitmark",
  "version": "1.0",
  "description": "Tab manager inspired by git",
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "options_page": "/backend/options.html",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "background": {
      "page": "/backend/background.html",
      "persistent": false
    },
  "browser_action": {
      "default_popup": "/backend/popup.html",
      "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png",
          "128": "images/get_started128.png"
    }
  },
  "manifest_version": 2
}
